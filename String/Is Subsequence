class Solution {
    public boolean isSubsequence(String s, String t) {
       
        boolean f = false;
        char[] a = s.toCharArray();
        char[] b = t.toCharArray();
        
        int j = 0;
        for (int i = 0; i < a.length; i++) {
            while (j < b.length && b[j] != a[i]) {
                j++; 
            }
            if (j == b.length) {
                return false; 
            }
            j++; 
        }
        return true; 
    }
}
