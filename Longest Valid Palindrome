class Solution {
    public boolean isPalindrome(String s) {
   if(s.isEmpty()){
    return true;
   }
   int st=0;
   int e=s.length()-1;
   while(st<=e){
     char c1 = s.charAt(st);
     char c2 = s.charAt(e);
     if(!Character. isLetterOrDigit(c1)){
        st++;
     }
     else if(!Character. isLetterOrDigit(c2)){
        e--;
     }
     else{
        if(Character.toLowerCase(c1) != Character.toLowerCase(c2)){
            return false;
            
        }
        st++;
        e--;
     }
     }
     return true;
}
}
