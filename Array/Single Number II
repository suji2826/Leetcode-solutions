class Solution {
    public int singleNumber(int[] nums) {
        int n = nums.length;
        HashMap<Integer,Integer> map = new HashMap<>();
        for(int a:nums){
             map.put(a,map.getOrDefault(a,0)+1);
        }
        for(int i=0;i<n;i++){
           if(map.get(nums[i])==1){
            return nums[i];
           }
        }
        return -1;
    }
}
